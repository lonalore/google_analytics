<?xml version="1.0" encoding="utf-8"?>
<e107Plugin name="Google Analytics" lan="LAN_PLUGIN_GOOGLE_ANALYTICS_NAME" version="1.0-beta2" date="2016-03-08" compatibility="2.0" installRequired="true">
    <author name="lonalore" url="https://github.com/lonalore/google_analytics"/>
    <summary lan="LAN_PLUGIN_GOOGLE_ANALYTICS_SUMM">Google Analytics</summary>
    <description lan="LAN_PLUGIN_GOOGLE_ANALYTICS_DESC">Adds the Google Analytics web statistics tracking system to your website.</description>
    <keywords>
        <word>google</word>
        <word>analytics</word>
    </keywords>
    <category>tools</category>
    <copyright></copyright>
    <adminLinks>
        <link url="admin_config.php" description="" icon="images/icon_32.png" iconSmall="images/icon_16.png" primary="true">LAN_CONFIGURE</link>
    </adminLinks>
    <pluginPrefs>
        <pref name='account'></pref>
        <pref name='domain_mode'>0</pref>
        <pref name='cross_domains'></pref>
        <pref name='visibility_pages'>0</pref>
        <pref name='pages'>e107_admin/*</pref>
        <pref name='visibility_roles'>0</pref>
        <pref name='track_user_id'>0</pref>
        <pref name='track_outbound'>1</pref>
        <pref name='track_mailto'>1</pref>
        <pref name='track_files'>1</pref>
        <pref name='track_files_extensions'>7z|aac|arc|arj|asf|asx|avi|bin|csv|doc(x|m)?|dot(x|m)?|exe|flv|gif|gz|gzip|hqx|jar|jpe?g|js|mp(2|3|4|e?g)|mov(ie)?|msi|msp|pdf|phps|png|ppt(x|m)?|pot(x|m)?|pps(x|m)?|ppam|sld(x|m)?|thmx|qtm?|ra(m|r)?|sea|sit|tar|tgz|torrent|txt|wav|wma|wmv|wpd|xls(x|m|b)?|xlt(x|m)|xlam|xml|z|zip</pref>
        <pref name='track_link_id'>0</pref>
        <pref name='track_url_fragments'>0</pref>
        <pref name='track_adsense'>0</pref>
        <pref name='track_double_click'>0</pref>
        <pref name='tracker_anonymize_ip'>1</pref>
        <pref name='privacy_do_not_track'>1</pref>
        <pref name='cache'>0</pref>
        <pref name='debug'>0</pref>
        <pref name='last_cache'>0</pref>
        <pref name='private_key'></pref>
    </pluginPrefs>
</e107Plugin>
